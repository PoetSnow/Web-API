<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>

    <!-- Css -->
    @*<link href="~/Content/Order/main.css" rel="stylesheet" />*@
    <link href="~/Content/ScanOrder/global.css" rel="stylesheet" />
    <link href="~/Scripts/ScanOrder/mui/css/mui.min.css" rel="stylesheet" />
    <link href="~/Scripts/ScanOrder/mui/css/mui.picker.all.css" rel="stylesheet" />
    <link href="~/Scripts/ScanOrder/layer_mobile/need/layer.css" rel="stylesheet" />
    <link href="~/Scripts/ScanOrder/swiper-4.4.1/css/swiper.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/ScanOrder/layout.css" rel="stylesheet" />
    <style>
        .ordersys-back {
            color: #FFFFFF !important;
        }
    </style>
    @RenderSection("Css", required: false)
</head>
<body>
    <header class="mui-bar mui-bar-nav">
        <a class=" mui-icon mui-icon-left-nav mui-pull-left ordersys-back" onclick="back()"></a>
        @*<a class=" mui-icon mui-icon-left-nav mui-pull-left ordersys-back"></a>*@
        <h1 class="mui-title">@ViewBag.Title </h1>
    </header>
    <div class="layout mui-content">
        @RenderBody()
    </div>

    <script type="text/javascript" src="~/Scripts/ScanOrder/mui/js/mui.min.js"></script>
    <script type="text/javascript" src="~/Scripts/ScanOrder/mui/js/mui.picker.all.js"></script>
    <script type="text/javascript" src="~/Scripts/ScanOrder/jquery-3.3.1.min.js"></script>
    <script type="text/javascript" src="~/Scripts/ScanOrder/layer_mobile/layer.js"></script>
    <script type="text/javascript" src="~/Scripts/ScanOrder/swiper-4.4.1/js/swiper.min.js"></script>
    <script type="text/javascript" src="~/Scripts/ScanOrder/globol.js"></script>

    @*<script src="~/Scripts/ScanOrder/juicer-min.js"></script>*@
    @RenderSection("Scripts", required: false)
    <script>

        layer.msg = function (msg, time) {
            if (time == undefined) {
                time = 1;
            }
            layer.open({
                content: msg
                , skin: 'msg'
                , time: time
                , shade: 'background-color: rgba(0,0,0,.3)'
            });
        }

        function back() {
            history.back();
        }
    </script>
</body>
</html>